    "babel-preset-stage-3": "6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "bufferutil": "^3.0.3",
    "chalk": "^2.3.0",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "compression-webpack-plugin": "^1.0.1",
