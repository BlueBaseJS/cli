{
  "name": "@blueeast/bluerain-cli-engine-web",
  "description": "BlueRain on Web",
  "author": "BlueEast Team <code@blueeast.com>",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "https://github.com/BlueEastCode/bluerain-cli",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "start": "DEBUG=*,-follow-redirects ts-node ./src/test.ts",
    "clean": "rimraf coverage dist tmp",
    "watch": "tsc -w -p tsconfig.json",
    "watch:test": "jest --watch",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test:only": "jest --coverage",
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "npm run lint",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "commit": "git-cz",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && opn dist/docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out dist/docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json dist/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d dist/docs",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "all": "run-s reset test cov:check doc:html",
    "prepare-release": "run-s all version doc:publish"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@blueeast/bluerain-cli-core": "../core",
    "@blueeast/bluerain-os": "^3.13.6",
    "compression": "^1.7.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "hpp": "^0.2.2",
    "offline-plugin": "^5.0.5",
    "react-helmet": "^5.2.0",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@blueeast/tslint-config-blueeast": "^0.5.6",
    "@types/compression": "^0.0.36",
    "@types/debug": "^0.0.30",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.38",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^23.1.6",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/react-helmet": "^5.0.7",
    "@types/serialize-javascript": "^1.3.2",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^4.4.9",
    "codecov": "^3.0.2",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "gh-pages": "^1.2.0",
    "jest": "^23.4.1",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^3.1.0",
    "prettier": "^1.13.4",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "standard-version": "^4.4.0",
    "trash-cli": "^1.4.0",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-immutable": "^4.5.4",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1",
    "webpack": "^4.16.4"
  },
  "peerDependencies": {
    "react": "^16.*",
    "react-dom": "^16.*"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": true,
    "trailingComma": "es5"
  }
}
